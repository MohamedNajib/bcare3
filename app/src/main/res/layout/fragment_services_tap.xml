        <com.example.fontutil.TextViewCustomFont
            android:id="@+id/TV_Foldedowel"
            style="@style/her"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="العناية بالأضافر"
            app:font_name="@string/VIP_Rawy_Regular"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/IV_Foldedowel" />

    </android.support.constraint.ConstraintLayout>


    <com.example.fontutil.TextViewCustomFont
        android:id="@+id/textViewCustomFont37"
