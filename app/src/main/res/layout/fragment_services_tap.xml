        android:id="@+id/CL_ServicesHandMirror"
        android:layout_width="@dimen/dimension70"
        android:layout_height="@dimen/dimension70"
        android:background="@drawable/hear"
        app:layout_constraintBottom_toBottomOf="@+id/CL_ServicesCucumberSlicesOnFace"
        app:layout_constraintEnd_toStartOf="@+id/view7"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/CL_ServicesCucumberSlicesOnFace"
