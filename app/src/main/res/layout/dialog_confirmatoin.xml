<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="269dp"
    android:layout_height="253dp"
    android:layout_gravity="center"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    app:cardElevation="@dimen/dimension4"
    app:cardCornerRadius="@dimen/dimension16"
    app:cardBackgroundColor="@android:color/white">

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginStart="16dp"
            android:layout_marginTop="@dimen/dimension45"
            android:layout_marginEnd="@dimen/dimension16"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_dialog_gro" />

        <com.example.fontutil.TextViewCustomFont
            android:id="@+id/TV_successful"
            style="@style/dialog"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="@dimen/dimension8"
            android:layout_marginEnd="@dimen/dimension16"
            app:font_name="@string/VIP_Rawy_Bold_Bold"
            android:text="@string/Booking_successful"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView5" />


        <com.example.fontutil.ButtonCustomFont
            android:id="@+id/BTN_DialogConfirmation"
            style="@style/Specialists"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="@dimen/dimension8"
            android:layout_marginEnd="@dimen/dimension16"
            android:layout_marginBottom="@dimen/dimension16"
            android:text="@string/title_dashboard"
            android:textAllCaps="false"
            app:font_name="@string/VIP_Rawy_Bold_Bold"
            android:background="@drawable/dialog_ptn"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/TV_successful" />

    </android.support.constraint.ConstraintLayout>

</android.support.v7.widget.CardView>

