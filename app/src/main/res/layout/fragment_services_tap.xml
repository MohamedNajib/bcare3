        app:layout_constraintTop_toBottomOf="@+id/CL_ServicesHairClipper" />

    <View
        android:id="@+id/view6"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:background="#A0ACC0"
        app:layout_constraintBottom_toBottomOf="@+id/textViewCustomFont30"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textViewCustomFont30"
        app:layout_constraintTop_toTopOf="@+id/textViewCustomFont30" />


