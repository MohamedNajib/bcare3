        app:layout_constraintEnd_toStartOf="@+id/CL_ServicesFlatIron"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/CL_ServicesHairClipper"
        app:layout_constraintTop_toBottomOf="@+id/spinner"
        app:layout_constraintTop_toTopOf="@+id/CL_ServicesHairClipper">

        <ImageView
            android:id="@+id/IV_hairdryer_facing"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toTopOf="@+id/TV_hairdryer_facing"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_chainStyle="packed"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_hairdryer_facing_a" />

        <com.example.fontutil.TextViewCustomFont
            android:id="@+id/TV_hairdryer_facing"
            style="@style/her"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="تصفيف الضعر"
            app:font_name="@string/VIP_Rawy_Regular"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/IV_hairdryer_facing" />

    </android.support.constraint.ConstraintLayout>

    <android.support.constraint.ConstraintLayout
        android:id="@+id/CL_ServicesFlatIron"
        android:layout_width="@dimen/dimension70"
        android:layout_height="@dimen/dimension70"
        android:background="@drawable/hear"
        app:layout_constraintBottom_toBottomOf="@+id/CL_ServicesHairdryerFacing"
        app:layout_constraintEnd_toStartOf="@+id/CL_ServicesRazor"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/CL_ServicesHairdryerFacing"
        app:layout_constraintTop_toTopOf="@+id/CL_ServicesHairdryerFacing">

        <ImageView
            android:id="@+id/IV_FlatIron"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toTopOf="@+id/TV_FlatIron"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_chainStyle="packed"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_flat_iron_a" />

        <com.example.fontutil.TextViewCustomFont
            android:id="@+id/TV_FlatIron"
            style="@style/her"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="كي الشعر"
            app:font_name="@string/VIP_Rawy_Regular"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
