        android:layout_width="@dimen/dimension70"
        android:layout_height="@dimen/dimension70"
        android:layout_marginTop="24dp"
        android:background="@drawable/hear"
        app:layout_constraintEnd_toStartOf="@+id/CL_ServicesHairdryerFacing"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_chainStyle="spread_inside"
        app:layout_constraintStart_toStartOf="@+id/spinner"
        app:layout_constraintTop_toBottomOf="@+id/spinner">

        <ImageView
            android:id="@+id/IV_HairClipper"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toTopOf="@+id/TV_HairClipper"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_chainStyle="packed"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_hair_clipper_a" />

        <com.example.fontutil.TextViewCustomFont
            android:id="@+id/TV_HairClipper"
            style="@style/her"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="قص الشعر"
            app:font_name="@string/VIP_Rawy_Regular"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/IV_HairClipper" />

    </android.support.constraint.ConstraintLayout>

    <android.support.constraint.ConstraintLayout
        android:id="@+id/CL_ServicesHairdryerFacing"
        android:layout_width="@dimen/dimension70"
